    "@types/jsonwebtoken": "^8.5.8",
    "@types/mongodb": "^3.5.25",
    "@types/node": "^14.0.13",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "eslint": "^7.2.0",
    "eslint-config-standard-with-typescript": "^18.0.2",
    "eslint-plugin-import": "^2.21.2",
